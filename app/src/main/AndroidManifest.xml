<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ricm.audifast">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_af"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_af_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AFMobile"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.requisitosProceso.RequisitosProceso"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AFMobile.NoActionBar"/>
        <activity
            android:name=".ui.procesosActa.ProcesosActa"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AFMobile.NoActionBar"/>
        <activity
            android:name=".ui.infoAuditoria.InfoAuditoria"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AFMobile.NoActionBar"/>
        <activity
            android:name=".ui.listaAuditorias.ListaAuditorias"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AFMobile.NoActionBar"/>
        <activity
            android:name=".ui.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>